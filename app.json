{
  "name": "todo",
  "displayName": "todo",
  "expo": {
    "name": "todo",
    "slug": "todo",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "android": {
      "useNextNotificationsApi": true,
      "icon":"./src/assets/icons/app-icon.png"
    },
    "notification":{
      "icon":"./src/assets/icons/ic_notification.png",
      "androidMode":"collapse"
    }
  }
}
